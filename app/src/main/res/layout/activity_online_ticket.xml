<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="onlineTicketVM" type="com.muhammadsapplication.app.modules.onlineticket.data.viewmodel.OnlineTicketVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearOnlineticket"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleorange_400">
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtMyQRCode"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_49pxh"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="match_parent"
            android:text="@{onlineTicketVM.onlineTicketModel.txtMyQRCode}"
            style="@style/txtDMSansbold20" tools:text="@string/msg_congratulations"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearQrCard"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_49pxh"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle_1">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearQrCard1"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_9">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectanglecopy"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleCopy"
                        android:layout_height="@dimen/_48pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="@dimen/_50pxh"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_20pxh}"
                        imageRes="@{@drawable/img_rectanglecopy}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectanglecopy"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnprofilename"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_weight="0.59"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtProfilename"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{onlineTicketVM.onlineTicketModel.txtProfilename}"
                            style="@style/txtDMSansbold12" tools:text="@string/lbl_profile_name"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHaileySanders"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_3pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent"
                            android:lineSpacingExtra="@dimen/_5pxh"
                            android:singleLine="true"
                            android:text="@{onlineTicketVM.onlineTicketModel.txtHaileySanders}"
                            style="@style/txtDMSansbold16" tools:text="@string/lbl_m_hadi"/>
                    </LinearLayout>
                </LinearLayout>
                <View android:background="@color/gray_301"
                    android:id="@+id/lineLineCopy"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_287pxh"/>
                <ImageView android:id="@+id/imageScannerCard"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_63pxh"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="@dimen/_256pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_scannercard"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_scannercard"/>
                <FrameLayout android:id="@+id/frameStackqrcode"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_137pxv"
                    android:layout_marginBottom="@dimen/_44pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_37pxv" android:layout_width="@dimen/_142pxh">
                    <ImageView android:id="@+id/imageQRcode"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_137pxh"
                        android:layout_width="@dimen/_142pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_qrcode"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_qrcode"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnF"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_29pxv"
                        android:layout_marginEnd="@dimen/_44pxh"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_8pxh"
                        android:paddingBottom="@dimen/_9pxv"
                        android:paddingEnd="@dimen/_18pxh"
                        android:paddingStart="@dimen/_19pxh"
                        android:paddingTop="@dimen/_9pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_f"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_f"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnlightbulb"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_49pxh"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylered_301cornerRadius">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowlightbulb"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_19"
                    android:id="@+id/btnLightbulb"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_width="@dimen/_50pxh"
                    android:paddingHorizontal="@dimen/_14pxh"
                    android:paddingVertical="@dimen/_14pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_lightbulb" tools:ignore="ContentDescription"/>
                <TextView android:id="@+id/txtSimplyshowthe"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.79"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:text="@{onlineTicketVM.onlineTicketModel.txtSimplyshowthe}"
                    style="@style/txtDMSansbold14" tools:text="@string/msg_simply_show_the"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMoredetailsCo"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_81pxh"
                android:layout_marginStart="@dimen/_81pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{onlineTicketVM.onlineTicketModel.txtMoredetailsCo}"
                style="@style/txtDMSansbold15" tools:text="@string/lbl_more_details"/>
        </LinearLayout>
        <View
            android:background="@drawable/rectangle_bg_gray_101_radius_2"
            android:id="@+id/lineRectangleCopyOne"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_4pxv"
            android:layout_marginEnd="@dimen/_49pxh"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_21pxv" android:layout_width="@dimen/_123pxh"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnBackToHome"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_39pxv"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_49pxh"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_29pxv"
            android:layout_width="@dimen/_162pxh"
            android:lineSpacingExtra="@dimen/_5pxh"
            android:paddingBottom="@dimen/_9pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_9pxv" android:singleLine="true"
            android:text="@string/lbl_back_to_home"
            android:textAllCaps="false" style="@style/btnSolidRounded_3" tools:text="@string/lbl_back_to_home"/>
    </LinearLayout>
</layout>
