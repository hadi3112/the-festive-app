<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="gettingStartedVM" type="com.muhammadsapplication.app.modules.gettingstarted.data.viewmodel.GettingStartedVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStacktosetupyour"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_63pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStackarrowleft"
                android:layout_gravity="start"
                android:layout_height="@dimen/_188pxv" android:layout_width="@dimen/_329pxh">
                <LinearLayout android:id="@+id/linearActionBarwith"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <View
                        android:background="@drawable/rectangle_bg_bluegray_100_11_radius_20"
                        android:id="@+id/viewRectangle39423"
                        android:layout_height="@dimen/_148pxv"
                        android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_329pxh"/>
                </LinearLayout>
                <ImageView android:id="@+id/imagePhoneWithMapic"
                    android:layout_gravity="bottom|center"
                    android:layout_height="@dimen/_145pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_93pxh"
                    android:layout_marginStart="@dimen/_93pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_141pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_phonewithmapic}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_phonewithmapic"/>
            </FrameLayout>
            <TextView android:gravity="start"
                android:id="@+id/txtTosetupyour"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.05"
                android:text="@{gettingStartedVM.gettingStartedModel.txtTosetupyour}"
                style="@style/txtAirbnbCerealAppbold22" tools:text="@string/msg_to_set_up_your"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowgetstartedbutt"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGetStartedbutt"
                    android:layout_height="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_38pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_getstartedbutt"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_getstartedbutt"/>
                <TextView android:id="@+id/txtVerificationEn"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_weight="0.82"
                    android:layout_width="0dp"
                    android:letterSpacing="0.05"
                    android:text="@{gettingStartedVM.gettingStartedModel.txtVerificationEn}"
                    style="@style/txtPoppinsmedium16" tools:text="@string/msg_verification_en"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowsettingsicon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageSettingsicon"
                    android:layout_height="@dimen/_38pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_38pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settingsicon"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settingsicon"/>
                <TextView android:id="@+id/txtYourPreference"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:letterSpacing="0.05"
                    android:text="@{gettingStartedVM.gettingStartedModel.txtYourPreference}"
                    style="@style/txtPoppinsmedium16" tools:text="@string/msg_your_preference"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameButtonColor"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_48pxv"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_57pxv" android:layout_width="@dimen/_269pxh">
                <LinearLayout android:id="@+id/linearColumnarrowright"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleyellow_800cornerRadius">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_orange_700_radius_16_55"
                        android:id="@+id/btnArrowright"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_33pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_32pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrowright" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContinue"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_10pxh"
                    android:layout_marginVertical="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.05"
                    android:singleLine="true"
                    android:text="@{gettingStartedVM.gettingStartedModel.txtContinue}"
                    android:textAllCaps="true"
                    style="@style/txtPoppinsbold18" tools:text="@string/lbl_get_started"/>
            </FrameLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
