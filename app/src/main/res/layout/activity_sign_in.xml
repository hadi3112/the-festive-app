<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signInVM" type="com.muhammadsapplication.app.modules.signin.data.viewmodel.SignInVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignin"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnfestiveicon"
            android:layout_gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_38pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_28pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imagefestiveicon"
                android:layout_gravity="start"
                android:layout_height="@dimen/_99pxh"
                android:layout_marginEnd="@dimen/_115pxh"
                android:layout_marginStart="@dimen/_115pxh"
                android:layout_width="@dimen/_69pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_festiveicon}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_festiveicon"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtfestive"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signInVM.signInModel.txtfestive}"
                style="@style/txtAirbnbCerealAppmedium35" tools:text="@string/lbl_festive"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignin"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signInVM.signInModel.txtSignin}"
                style="@style/txtAirbnbCerealAppmedium24" tools:text="@string/lbl_sign_in"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_12"
                android:drawablePadding="@dimen/_16pxh"
                android:drawableStart="@drawable/img_mail"
                android:ellipsize="end"
                android:hint="@string/lbl_abc_email_com"
                android:id="@+id/etEmail"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signInVM.signInModel.etEmailValue}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_12"
                android:drawableEnd="@drawable/img_hidden"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_lock"
                android:ellipsize="end"
                android:hint="@string/lbl_your_password"
                android:id="@+id/etYourPasswordMas"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signInVM.signInModel.etYourPasswordMasValue}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <LinearLayout android:id="@+id/linearRowforgotpassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowrememberme"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.57"
                    android:layout_width="0dp" android:orientation="horizontal">

                    <com.google.android.material.switchmaterial.SwitchMaterial
                        android:id="@+id/switchMaterialSwitch"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.25" android:layout_width="0dp"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRememberMe"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.75"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{signInVM.signInModel.txtRememberMe}"
                        style="@style/txtAirbnbCerealAppmedium14_2" tools:text="@string/lbl_remember_me"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtForgotPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_80pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.43"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtForgotPassword}"
                    style="@style/txtAirbnbCerealAppmedium12_3"
                    textUnderline="true" tools:text="@string/msg_forgot_password"
                    tools:ignore="TextSizeCheck"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameSignInButton"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_58pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_37pxv" android:layout_width="@dimen/_271pxh">
                <LinearLayout android:id="@+id/linearColumnarrowright"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleyellow_1">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_orange_700_radius_15"
                        android:id="@+id/btnArrowright"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginHorizontal="@dimen/_14pxh"
                        android:layout_marginVertical="@dimen/_14pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrowright" tools:ignore="ContentDescription"
                        android:contentDescription="NULL"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContinue"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.05"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtContinue}"
                    android:textAllCaps="true"
                    style="@style/txtAirbnbCerealAppmedium16" tools:text="@string/lbl_sign_in"/>
            </FrameLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtOR"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{signInVM.signInModel.txtOR}"
                style="@style/txtAirbnbCerealAppmedium16_1" tools:text="@string/lbl_or"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowgoogle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <ImageView android:id="@+id/imageGoogle"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_google"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoginwithGoog"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtLoginwithGoog}"
                    style="@style/txtAirbnbCerealAppmedium16_2" tools:text="@string/msg_login_with_goog"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowfacebook"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <ImageButton
                    android:background="@drawable/rectangle_bg_blue_a400_radius_15_38"
                    android:id="@+id/btnFacebook"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_facebook" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoginwithFace"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{signInVM.signInModel.txtLoginwithFace}"
                    style="@style/txtAirbnbCerealAppmedium16_2" tools:text="@string/msg_login_with_face"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtConfirmation"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{signInVM.signInModel.txtConfirmation}"
                style="@style/txtAirbnbCerealAppmedium15_1" tools:text="@string/msg_don_t_have_an_a"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearBgMask"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowellipsesixtysix"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseSixtySix"
                    android:layout_height="@dimen/_173pxh"
                    android:layout_width="@dimen/_135pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse66}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse66"/>
                <ImageView android:id="@+id/imageEllipseSixtyNine"
                    android:layout_height="@dimen/_134pxh"
                    android:layout_marginBottom="@dimen/_39pxv"
                    android:layout_marginStart="@dimen/_63pxh"
                    android:layout_width="@dimen/_177pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse69}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse69"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowellipseseventyone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_321pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseSeventyOne"
                    android:layout_height="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_249pxv"
                    android:layout_width="@dimen/_82pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse71}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse71"/>
                <ImageView android:id="@+id/imageEllipseSeventy"
                    android:layout_height="@dimen/_317pxh"
                    android:layout_marginStart="@dimen/_227pxh"
                    android:layout_width="@dimen/_66pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse70}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse70"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
