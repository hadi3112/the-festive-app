<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpVM" type="com.muhammadsapplication.app.modules.signup.data.viewmodel.SignUpVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignup"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearBgMask2"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowellipsesixtysix"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseSixtySix"
                    android:layout_height="@dimen/_173pxh"
                    android:layout_width="@dimen/_135pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse66_173x135}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse66_173x135"/>
                <ImageView android:id="@+id/imageEllipseSixtyNine"
                    android:layout_height="@dimen/_134pxh"
                    android:layout_marginBottom="@dimen/_39pxv"
                    android:layout_marginStart="@dimen/_63pxh"
                    android:layout_width="@dimen/_177pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse69_134x177}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse69_134x177"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowellipseseventyone"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_321pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageEllipseSeventyOne"
                    android:layout_height="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_249pxv"
                    android:layout_width="@dimen/_82pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse71_68x82}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse71_68x82"/>
                <ImageView android:id="@+id/imageEllipseSeventy"
                    android:layout_height="@dimen/_317pxh"
                    android:layout_marginStart="@dimen/_227pxh"
                    android:layout_width="@dimen/_66pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_ellipse70_317x66}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse70_317x66"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearColumnsignup"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_38pxv"
            android:layout_marginEnd="@dimen/_29pxh"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_38pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignup"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtSignup}"
                style="@style/txtAirbnbCerealAppmedium24_1" tools:text="@string/lbl_sign_up"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_12"
                android:drawablePadding="@dimen/_18pxh"
                android:drawableStart="@drawable/img_user"
                android:ellipsize="end"
                android:hint="@string/lbl_full_name"
                android:id="@+id/etGroup33330" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={signUpVM.signUpModel.etGroup33330Value}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_12"
                android:drawablePadding="@dimen/_16pxh"
                android:drawableStart="@drawable/img_mail"
                android:ellipsize="end"
                android:hint="@string/lbl_abc_email_com"
                android:id="@+id/etEmail"
                android:inputType="textEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signUpVM.signUpModel.etEmailValue}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_12"
                android:drawableEnd="@drawable/img_hidden"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_lock"
                android:ellipsize="end"
                android:hint="@string/lbl_your_password"
                android:id="@+id/etGroup33330One"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signUpVM.signUpModel.etGroup33330OneValue}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <EditText
                android:background="@drawable/rectangle_bg_white_a700_border_gray_300_radius_12"
                android:drawableEnd="@drawable/img_hidden"
                android:drawablePadding="@dimen/_30pxh"
                android:drawableStart="@drawable/img_lock"
                android:ellipsize="end"
                android:hint="@string/msg_confirm_passwor"
                android:id="@+id/etGroup33330Two"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_19pxh"
                android:paddingStart="@dimen/_18pxh"
                android:paddingTop="@dimen/_19pxv"
                android:singleLine="true"
                android:text="@={signUpVM.signUpModel.etGroup33330TwoValue}"
                android:textColorHint="@color/gray_600"
                style="@style/etSolidRoundedOutline" tools:ignore="Autofill"/>
            <FrameLayout android:id="@+id/frameSignUpButton"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_58pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_40pxv" android:layout_width="@dimen/_271pxh">
                <LinearLayout android:id="@+id/linearColumnarrowright"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStyleyellow_1">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_orange_700_radius_15"
                        android:id="@+id/btnArrowright"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginHorizontal="@dimen/_14pxh"
                        android:layout_marginVertical="@dimen/_14pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_arrowright" tools:ignore="ContentDescription"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtContinue"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.05"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtContinue}"
                    android:textAllCaps="true"
                    style="@style/txtAirbnbCerealAppmedium16_3" tools:text="@string/lbl_sign_up"/>
            </FrameLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtOR"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_width="wrap_content"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtOR}"
                style="@style/txtAirbnbCerealAppmedium16_1" tools:text="@string/lbl_or"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowgoogle"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <ImageView android:id="@+id/imageGoogle"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_25pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_google"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_google"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoginwithGoog"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_45pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtLoginwithGoog}"
                    style="@style/txtAirbnbCerealAppmedium16_2" tools:text="@string/msg_login_with_goog"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearRowfacebook"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_1">
                <FrameLayout android:id="@+id/frameStackfacebook"
                    android:layout_height="@dimen/_30pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_30pxh" style="@style/groupStyleblue_A400cornerRadius">
                    <ImageView android:id="@+id/imageFacebook"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_7pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_facebook"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_facebook"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLoginwithFace"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_5pxh"
                    android:singleLine="true"
                    android:text="@{signUpVM.signUpModel.txtLoginwithFace}"
                    style="@style/txtAirbnbCerealAppmedium16_2" tools:text="@string/msg_login_with_face"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtConfirmation"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@{signUpVM.signUpModel.txtConfirmation}"
                style="@style/txtAirbnbCerealAppmedium15_1" tools:text="@string/msg_already_have_an"/>
        </LinearLayout>
    </FrameLayout>
</layout>
